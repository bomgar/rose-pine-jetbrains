  "icons": {
    "ColorPalette": {
      "Actions.Blue": "$foam",
      "Actions.Green": "$pine",
      "Actions.Grey": "$subtle",
      "Actions.GreyInline": "$muted",
      "Actions.GreyInline.Dark": "$text",
      "Actions.Red": "$love",
      "Actions.Yellow": "$gold",
      "Objects.BlackText": "$text",
      "Objects.Blue": "$foam",
      "Objects.Green": "$pine",
      "Objects.Grey": "$subtle",
      "Objects.Pink": "$rose",
      "Objects.Purple": "$iris",
      "Objects.Red": "$love",
      "Objects.RedStatus": "$love",
      "Objects.Yellow": "$gold",
      "Objects.YellowDark": "$gold",
      "Checkbox.Background.Default": "$subtle",
      "Checkbox.Background.Default.Dark": "$subtle",
      "Checkbox.Background.Disabled": "$subtle",
      "Checkbox.Background.Disabled.Dark": "$subtle",
      "Checkbox.Background.Selected": "$subtle",
      "Checkbox.Background.Selected.Dark": "$subtle",
      "Checkbox.Border.Default": "$subtle",
      "Checkbox.Border.Disabled": "$subtle",
      "Checkbox.Border.Selected": "$subtle",
      "Checkbox.Foreground.Disabled": "$subtle",
      "Checkbox.Foreground.Disabled.Dark": "$subtle",
      "Checkbox.Foreground.Selected": "$subtle",
      "Checkbox.Foreground.Selected.Dark": "$subtle",
      "Tree.iconColor": "$text"
    }
  }
}
